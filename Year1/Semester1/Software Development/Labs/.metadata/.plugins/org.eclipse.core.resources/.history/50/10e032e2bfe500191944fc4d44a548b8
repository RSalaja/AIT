
public class MyFormat {

	public static void main(String[] args) 
	{
		System.out.println("PI = "+Math.PI);
		System.out.println("1/3 = "+(1.0/3.0));
		System.out.printf("Hourly Wage = €%17.2f\n",(1243/37.5));
		System.out.printf("Hourly Wage = €%17.2f\n",(1243/37.5));
		System.out.printf("Hourly Wage = €%-17.2f\n",(1243/37.5));
		System.out.printf("Hourly Wage = €%(17.2f\n",(1243/-37.5));
	
	}

}
